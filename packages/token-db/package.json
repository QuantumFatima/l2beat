{
  "name": "@l2beat/token-db",
  "version": "0.0.0",
  "type": "module",
  "main": "dist/main.js",
  "types": "dist/main.d.ts",
  "license": "MIT",
  "private": true,
  "scripts": {
    "build": "tsc",
    "clean": "rm -rf build",
    "dev": "tsx --no-warnings=ExperimentalWarning --watch src/main.ts",
    "db:export": "tsx src/scripts/export.ts",
    "db:seed": "tsx src/scripts/seed.ts",
    "format": "biome format .",
    "format:fix": "biome format --write .",
    "lint": "biome check --formatter-enabled=false .",
    "lint:fix": "biome check --formatter-enabled=false --apply .",
    "nanoid": "nanoid",
    "start": "tsx --no-warnings=ExperimentalWarning --watch src/queue.ts",
    "test": "vitest",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@bull-board/api": "^5.17.1",
    "@bull-board/express": "^5.17.1",
    "@l2beat/backend-tools": "^0.5.2",
    "@l2beat/database": "*",
    "@types/express": "^4.17.21",
    "bullmq": "^5.7.8",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "ioredis": "^5.4.1",
    "lodash": "^4.17.21",
    "nanoid": "^5.0.7",
    "papaparse": "^5.4.1",
    "parse-redis-url-simple": "^1.0.2",
    "postgres": "^3.4.4",
    "viem": "^2.10.1",
    "zod": "^3.23.6"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.7.3",
    "@types/lodash": "^4.17.1",
    "@types/node": "^20.12.10",
    "@types/papaparse": "^5.3.14",
    "npm-run-all": "^4.1.5",
    "prisma": "^5.13.0",
    "rimraf": "^5.0.5",
    "tsx": "^4.9.3",
    "type-fest": "^4.18.2",
    "typescript": "^5.4.5",
    "vitest": "^1.6.0"
  }
}
